{
  "kind": "singleType",
  "collectionName": "footers",
  "info": {
    "singularName": "footer",
    "pluralName": "footers",
    "displayName": "Footer",
    "description": "Global footer configuration"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "logo": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "description": {
      "type": "text"
    },
    "columns": {
      "type": "dynamiczone",
      "components": [
        "vandan-food.footer-section",
        "vandan-food.contact-section"
      ]
    },
    "copyrightText": {
      "type": "customField",
      "customField": "plugin::ckeditor5.CKEditor",
      "options": {
        "preset": "defaultHtml"
      }
    }
  }
}
